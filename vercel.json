{
    "name": "api_prueba_mercado_libre",
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
        "FIRST_NAME": "@first-name" ,
        "LAST_NAME": "@last-name"
    }
  }